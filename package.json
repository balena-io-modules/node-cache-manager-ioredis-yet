{
  "name": "cache-manager-redis-yet",
  "authors": [
    "Matthijs Dabroek <dabroek@gmail.com>",
    {
      "name": "Juan Aguilar Santillana",
      "email": "mhpoin@gmail.com"
    }
  ],
  "description": "Redis store for node-cache-manager updated",
  "version": "3.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/botika/node-cache-manager-redis-v4.git"
  },
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "test:watch": "vitest test.ts",
    "test": "pnpm test:watch --run",
    "release": "pnpm check && pnpm test && pnpm build && dotenv release-it",
    "fix": "pnpm lint && pnpm fmt",
    "check": "pnpm lint:check && pnpm fmt:check",
    "lint": "eslint --cache --fix '**/*.{ts,js}'",
    "fmt": "prettier --write '**/*.{md,prisma,json}'",
    "lint:check": "eslint --cache --max-warnings 0 '**/*.{ts,js}'",
    "fmt:check": "prettier --check '**/*.{md,json}'",
    "commitlint": "commitlint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@redis/bloom": "^1.0.2",
    "@redis/client": "^1.2.0",
    "@redis/graph": "^1.0.1",
    "@redis/json": "^1.0.3",
    "@redis/search": "^1.0.6",
    "@redis/time-series": "^1.0.3",
    "redis": "^4.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@release-it/conventional-changelog": "^5.0.0",
    "@types/cache-manager": "^4.0.1",
    "@types/node": "^18.6.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "cache-manager": "^4.1.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-prisma": "^4.1.0",
    "prisma": "^4.1.1",
    "release-it": "^15.2.0",
    "typescript": "^4.7.4",
    "vitest": "^0.20.3"
  },
  "engines": {
    "node": ">= 16"
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --fix",
    "*.{json,prisma,md,yml}": "prettier --write"
  }
}
